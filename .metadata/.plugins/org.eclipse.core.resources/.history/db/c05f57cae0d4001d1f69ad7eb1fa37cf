package chap21_stream;

import java.util.ArrayList;
import java.util.List;

public class _10_flatMap {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		List<String> strList = new ArrayList<String>();
		
		strList.add("abc");
		strList.add("def");
		
		strList.stream()
				.flatMap(str -> str)
		
	}

}
