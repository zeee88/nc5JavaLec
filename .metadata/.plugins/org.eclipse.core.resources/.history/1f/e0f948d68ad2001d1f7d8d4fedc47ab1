package chap19_multithread;

public class _13_multiThreadEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i = 1; i <= 10; i++ ) {
			if(i % 3 == 0) {
			}
		}	
		
		for(int i = 1; i <= 10; i++ ) {
			if(i % 3 == 1) {
			}
		}	
		
		for(int i = 1; i <= 10; i++ ) {
			if(i % 3 == 2) {
			}
		}	
			
		
		
		
	}

}
