package Homework_0330;

public class Student {

	public _03 name;
	public _03 sNum;
	public _03 major;
	
	
	
	//학생 정보 저장
	public void studentInfo(_03 name, _03 sNum, _03 major) {
		this.name = name;
		this.sNum = sNum;
		this.major = major;
		
	}

	
	//학생 정보 출력
	public void studentPrint() {
		System.out.println("이름: " +  name);
		System.out.println("학번: " + sNum);
		System.out.println("전공: " + major);
	}
	
}
