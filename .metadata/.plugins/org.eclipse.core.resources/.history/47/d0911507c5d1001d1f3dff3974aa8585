package chap19_multithread;

public class _04_joinMethod {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread t1 = new Thread() {
			@Override
			public void run() {
				for(int i = 6; i <= 10; i++) {
					System.out.println(i);
				}
			}
		};
		//t1 실행 대기 상태로 변경
		t1.start();
	}

}
