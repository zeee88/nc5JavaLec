package Homework_0330;

public class Book {

	public String title;
	public String writer;
	public String publisher;
	public int price;
	
	
	public void bookInfo(String title, String writer, String publisher, int price) {
		this.title = title;
		this.writer = writer;
		this.publisher = publisher;
		this.price = price;
	}
	
	
}
