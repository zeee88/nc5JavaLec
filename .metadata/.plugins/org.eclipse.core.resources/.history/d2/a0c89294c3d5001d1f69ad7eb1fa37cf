package etc;

class Mypoint{
	int x;
	int y;
	String getLocation() {
		return "x: " + x + "Y: " + y;
	}
}

class Mypoint3D extends Mypoint{
	int z;
	
	//부모의 getLocation()을 오버라이딩
	String getLocation() {
		return "x: " + x + "Y: " + y + "z: " + z;
	}
}


public class overriding {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Mypoint p = new Mypoint();
		
		p.x = 3;
		p.y = 5;
		p.z = 7;
		System.out.println(p.getLocation());
		
		
		
		
	}

}
