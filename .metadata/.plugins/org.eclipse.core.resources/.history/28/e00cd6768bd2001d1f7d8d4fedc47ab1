package chap19_multithread;

public class _13_multiThreadEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Thread t1 = new Thread();
		
		t1.start();
		
		try {
		for(int i = 1; i <= 10; i++ ) {
			if(i % 3 == 0) {
			}
		}
		}catch(Exception ie) {
			System.out.println(ie.getMessage());
			
		}
		
		
		for(int i = 1; i <= 10; i++ ) {
			if(i % 3 == 1) {
			}
		}	
		
		
		for(int i = 1; i <= 10; i++ ) {
			if(i % 3 == 2) {
			}
		}	
			
		
		
		
	}

}
