package chap19_multithread.clazz;

public class InterruptThread extends Thread{
	
	@Override
	public void run() {
		while(true) {
			System.out.println("스레드 실행됨");
			
			try {
				Thread.sleep(1);
			}catch(InterruptedException ie) {
				//리소스 정리
				System.out.println(ie.getMessage());
			}
		}
		System.out.println("스레드 종료");
	}
	
	
}
