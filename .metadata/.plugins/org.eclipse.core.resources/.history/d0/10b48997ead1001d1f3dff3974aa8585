package chap19_multithread.clazz;

public class SyncThread1 extends Thread{
//	private printNumWithSync pns;
	private printNumWithWaitNotifySync pns;
	
	public void setPns(printNumWithWaitNotifySync pns) {
		this.pns = pns;
	}
	
	public printNumWithSync getPns() {
		return pns;
	}

	@Override
	public void run() {
		pns.printNum1(10);
	}
}
