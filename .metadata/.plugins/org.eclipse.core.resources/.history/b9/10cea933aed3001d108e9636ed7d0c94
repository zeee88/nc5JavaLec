		List<Character> charList = new ArrayList <Character>();
		
		
		Stream <Character> chStream = charList.stream();
		
		Stream <Character> mapStream = chStream.map(m -> {m.toUpperCase(m);
														return m;		
														});
		mapStream.forEach(m -> System.out.println(m));
		}
		
		if('A' <= ch && ch <= 'Z') {
