package Homework_0330;

public class _03 {

	public _03 str1;
	public _03 str2;
	public _03 newStr;
	
	
	public void info(_03 str1, _03 str2, _03 newStr) {
		this.str1 = str1;
		this.str2 = str2;
		this.newStr = newStr;
	}
	
	
	public void nnew() {
		System.out.println("입력한 문자열의 합: " + newStr);
	}
	
	
}
