	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Calendar d1 = Calendar.getInstance();
		Calendar d2 = Calendar.getInstance();
		
		d2.set(2011, 2, 11);
		
		
		//getTimeInMill() : long타입으로 지정된 날짜를 밀리세컨드로 리
		
		
	}

}
