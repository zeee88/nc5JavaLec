package chap18_enum.enums;

public enum RGB {
	//상수명 RED("red", 255, 0, 0),
	//     GREEN("green", 0, 255, 0),
	//     BLUE("blue", 0, 0, 255)
	
	RED("red", 255, 0, 0),
	GREEN("green", 0, 255, 0),
	BLUE("blue", 0, 0, 255);
	
	private final String color;
	private final int i;
	private final int j;
	private final int k;
	
	RGB(String color, int i, int j, int k) {
	}
	
	
}
